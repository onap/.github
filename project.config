[access]
	inheritFrom = All-Projects
[submit]
	action = inherit
[access "refs/*"]
	owner = group saml/onap-gerrit-github-committers
	read = group GitHub Replication
[access "refs/heads/*"]
	label-Code-Review = deny -2..+2 group Release Engineering Team
	submit = deny group Release Engineering Team
[project]
	description = This repo contains required GitHub actions workflows
[plugin "its-jira"]
	enabled = true
